{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug hello_moveit node",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/hello_moveit/lib/hello_moveit/hello_moveit",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "AMENT_PREFIX_PATH",
          "value": "${workspaceFolder}/install"
        },
        {
          "name": "LD_LIBRARY_PATH",
          "value": "${workspaceFolder}/install/hello_moveit/lib"
        }
      ],
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build hello_moveit",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}